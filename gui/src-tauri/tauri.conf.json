{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "s3si-ts",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "sidecar": true,
        "scope": [
          {
            "name": "../binaries/s3si",
            "sidecar": true,
            "args": [
              "--daemon"
            ]
          }
        ],
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      },
      "fs": {
        "scope": [
          "$APPCONFIG",
          "$APPDATA",
          "$APPCACHE"
        ]
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "cn.imspace.s3si.ts",
      "targets": "all",
      "externalBin": [
        "../binaries/s3si"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "s3si.ts",
        "width": 800,
        "height": 600,
        "visible": false
      }
    ]
  }
}